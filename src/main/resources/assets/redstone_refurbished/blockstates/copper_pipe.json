{
  "multipart": [
    {
      "when": {
        "OR": [
          {
            "north": "none",
            "south": "none",
            "east": "none",
            "west": "none",
            "up": "none",
            "down": "none"
          },
          {
            "north": "side|up",
            "east": "side|up"
          },
          {
            "south": "side|up",
            "east": "side|up"
          },
          {
            "south": "side|up",
            "west": "side|up"
          },
          {
            "north": "side|up",
            "west": "side|up"
          }
        ]
      },
      "apply": {
        "model": "redstone_refurbished:block/copper_pipe_unconnected"
      }
    },
    {
      "when": {
        "north": "side|up"
      },
      "apply": {
        "model": "redstone_refurbished:block/copper_pipe_side"
      }
    },
    {
      "when": {
        "south": "side|up"
      },
      "apply": {
        "model": "redstone_refurbished:block/copper_pipe_side"
      }
    },
    {
      "when": {
        "east": "side|up"
      },
      "apply": {
        "model": "redstone_refurbished:block/copper_pipe_side",
        "y": 270
      }
    },
    {
      "when": {
        "west": "side|up"
      },
      "apply": {
        "model": "redstone_refurbished:block/copper_pipe_side",
        "y": 270
      }
    },
    {
      "when": {
        "up": "side|up"
      },
      "apply": {
        "model": "redstone_refurbished:block/copper_pipe_up",
        "y": 270
      }
    },
    {
      "when": {
        "down": "side|up"
      },
      "apply": {
        "model": "redstone_refurbished:block/copper_pipe_up",
        "y": 270
      }
    },
    {
      "when": {
        "north": "up"
      },
      "apply": {
        "model": "redstone_refurbished:block/copper_pipe_up"
      }
    },
    {
      "when": {
        "east": "up"
      },
      "apply": {
        "model": "redstone_refurbished:block/copper_pipe_up",
        "y": 90
      }
    },
    {
      "when": {
        "south": "up"
      },
      "apply": {
        "model": "redstone_refurbished:block/copper_pipe_up",
        "y": 180
      }
    },
    {
      "when": {
        "west": "up"
      },
      "apply": {
        "model": "redstone_refurbished:block/copper_pipe_up",
        "y": 270
      }
    },
    {
      "when": {
        "up": "up"
      },
      "apply": {
        "model": "redstone_refurbished:block/copper_pipe_up",
        "y": 270
      }
    },
    {
      "when": {
        "down": "up"
      },
      "apply": {
        "model": "redstone_refurbished:block/copper_pipe_up",
        "y": 270
      }
    }
  ]
}